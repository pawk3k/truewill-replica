{
  "$schema": "https://turborepo.org/schema.json",
  "baseBranch": "origin/main",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", "build/**", ".next/**"]
    },
    "clean": {
      "cache": false
    },
    "client#build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**"]
    },
    "dev": {
      "cache": false,
      "outputs": []
    },
    "lint": {
      "outputs": []
    },
    "server#build": {
      "dependsOn": ["^build"],
      "outputs": ["build/**"]
    },
    "storybook": {
      "outputs": []
    }
  }
}
